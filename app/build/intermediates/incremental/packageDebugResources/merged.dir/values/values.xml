<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:ns1="http://schemas.android.com/tools">
    <color name="black">#FF000000</color>
    <color name="color_button_start_clicked_gradient_bottom">#96DA00</color>
    <color name="color_button_start_clicked_gradient_center">#B2E602</color>
    <color name="color_button_start_clicked_gradient_top">#CCF104</color>
    <color name="color_button_start_clicked_stroke">#B8D700</color>
    <color name="color_button_start_default_gradient_bottom">#7FB32D</color>
    <color name="color_button_start_default_gradient_center">#99C231</color>
    <color name="color_button_start_default_gradient_top">#B5D336</color>
    <color name="color_button_start_default_stroke">#B8D437</color>
    <color name="color_icon_clicked">#80FFFFFF</color>
    <color name="color_icon_default">#FFF</color>
    <color name="color_shape_coin">#1C315A</color>
    <color name="color_shape_image">#383F52</color>
    <color name="color_toolbar">#1C2747</color>
    <color name="ic_launcher_background">#FFFFFF</color>
    <color name="purple_200">#FFBB86FC</color>
    <color name="purple_500">#FF6200EE</color>
    <color name="purple_700">#FF3700B3</color>
    <color name="teal_200">#FF03DAC5</color>
    <color name="teal_700">#FF018786</color>
    <color name="white">#FFFFFFFF</color>
    <string name="app_name">4 Фотки 1 Слово</string>
    <string name="hello_blank_fragment">Hello blank fragment</string>
    <string name="text_continue">Дальше</string>
    <string name="text_greetings">ВЕЛИКОЛЕПНО</string>
    <string name="text_play">Играть</string>
    <string name="text_settings">Настройки</string>
    <string name="text_sound">Звук</string>
    <style name="MatchParent">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
    </style>
    <style name="StyleContainerImages">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:background">@drawable/shape_image</item>
        <item name="android:padding">@dimen/_6sdp</item>
    </style>
    <style name="StyleContinueButton" parent="StyleStartButton">
        <item name="android:layout_width">@dimen/_172sdp</item>
    </style>
    <style name="StyleIcon">
        <item name="android:layout_width">@dimen/_20sdp</item>
        <item name="android:layout_height">@dimen/_20sdp</item>
        <item name="android:layout_marginStart">@dimen/_8sdp</item>
    </style>
    <style name="StyleIconLeft" parent="StyleIcon">
        <item name="android:layout_marginEnd">@dimen/_8sdp</item>
    </style>
    <style name="StyleImages">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:scaleType">centerCrop</item>
    </style>
    <style name="StyleSettingsSwitch">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_marginEnd">@dimen/_16sdp</item>
    </style>
    <style name="StyleSettingsText">
        <item name="android:textSize">@dimen/_16ssp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">@dimen/_16sdp</item>
        <item name="android:fontFamily">@font/museosans700</item>
        <item name="android:textColor">@color/white</item>
    </style>
    <style name="StyleStartButton">
        <item name="android:shadowColor">#0A3A04</item>
        <item name="android:shadowDx">1.6</item>
        <item name="android:shadowDy">1.6</item>
        <item name="android:shadowRadius">8</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">@dimen/_48sdp</item>
        <item name="android:background">@drawable/selector_button_start</item>
        <item name="android:fontFamily">@font/museosans900</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/_24ssp</item>
        <item name="android:layout_marginStart">@dimen/_24sdp</item>
        <item name="android:layout_marginEnd">@dimen/_24sdp</item>
    </style>
    <style name="StyleTextCoins">
        <item name="android:layout_marginTop">@dimen/_4sdp</item>
        <item name="android:layout_marginBottom">@dimen/_4sdp</item>
        <item name="android:layout_marginEnd">@dimen/_8sdp</item>
        <item name="android:textSize">@dimen/_16ssp</item>
        <item name="android:fontFamily">@font/museosans900</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingStart">@dimen/_8sdp</item>
        <item name="android:paddingEnd">@dimen/_8sdp</item>
        <item name="android:drawablePadding">@dimen/_8sdp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:background">@drawable/shape_coin</item>
    </style>
    <style name="StyleTextCollectedCoins" parent="StyleTextGreetings">
        <item name="android:shadowColor">#3F413F</item>
        <item name="android:shadowDx">1.6</item>
        <item name="android:shadowDy">1.6</item>
        <item name="android:shadowRadius">8</item>
        <item name="android:textColor">#FFD379</item>
        <item name="android:textSize">@dimen/_32ssp</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="StyleTextGreetings">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/_56sdp</item>
        <item name="android:fontFamily">@font/museosans900</item>
        <item name="android:shadowColor">#6DCB61</item>
        <item name="android:shadowDx">1.6</item>
        <item name="android:shadowDy">1.6</item>
        <item name="android:shadowRadius">8</item>
        <item name="android:textColor">#2CD22C</item>
        <item name="android:textSize">@dimen/_32ssp</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="TextViewAnswer">
        <item name="android:layout_width">@dimen/_32sdp</item>
        <item name="android:layout_height">@dimen/_32sdp</item>
        <item name="android:background">@drawable/ic_answer_button</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">@dimen/_16ssp</item>
        <item name="android:textAlignment">center</item>
        <item name="android:textStyle">bold</item>
        <item name="fontFamily">@font/museosans900</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:enabled">false</item>
        <item name="android:layout_marginStart">@dimen/_2sdp</item>
        <item name="android:layout_marginEnd">@dimen/_2sdp</item>
    </style>
    <style name="TextViewVariant">
        <item name="android:clickable">true</item>
        <item name="android:focusable">true</item>
        <item name="fontFamily">@font/museosans900</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:background">@drawable/selector_variants</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">@dimen/_16ssp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/black</item>
    </style>
    <style name="Theme.FindWordApp" parent="Theme.MaterialComponents.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/color_toolbar</item>
        <item name="colorPrimaryVariant">@color/color_toolbar</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" ns1:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>
    <style name="WrapContent">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
</resources>